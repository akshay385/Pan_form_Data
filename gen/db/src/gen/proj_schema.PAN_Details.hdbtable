COLUMN TABLE proj_schema_PAN_Details (
  PAN_Number NVARCHAR(5000) NOT NULL DEFAULT 'def',
  SBG NVARCHAR(5000),
  SBU NVARCHAR(5000),
  BUORPurchasing_Group NVARCHAR(5000),
  Plant_Code NVARCHAR(5000),
  Project_Description NVARCHAR(5000),
  PR_NumberBKTsBKT NVARCHAR(5000),
  Subject_of_ProposalOROrder NVARCHAR(5000),
  Previous_PAN_References NVARCHAR(5000),
  Split_OrderORNo_of_vendors NVARCHAR(5000),
  SOP_Type NVARCHAR(5000),
  Order_Type_OR_Document_tyFuuidpe NVARCHAR(5000),
  Asset_Type NVARCHAR(5000),
  Nature_of_Transaction NVARCHAR(5000),
  Order_Location_OR_Plant NVARCHAR(5000),
  Base_line_spend NVARCHAR(5000),
  Project_CurrencyORBase_Currency NVARCHAR(5000),
  Order_CurrencyORBid_currency NVARCHAR(5000),
  Final_proposed_Value NVARCHAR(5000),
  Savings_achieved_btw_initial_and_final_quote NVARCHAR(5000),
  Savings_against_base_line_spend_of_RFP NVARCHAR(5000),
  Number_of_Vendors_Shortlisted_for_RFP NVARCHAR(5000),
  Number_of_Vendors_Technically_Qualified NVARCHAR(5000),
  Required_at_Site_Date NVARCHAR(5000),
  RFP_Number NVARCHAR(5000),
  RFP_Publish_Date NVARCHAR(5000),
  Time_Taken_for_FinalizationDASHIn_DAYS NVARCHAR(5000),
  status NVARCHAR(5000),
  statusInd INTEGER,
  created_by NVARCHAR(5000),
  task_id NVARCHAR(5000),
  type NVARCHAR(5000),
  status_a NVARCHAR(5000),
  switch_control BOOLEAN DEFAULT FALSE,
  ProjectId NVARCHAR(5000),
  number_of_vendors_invited NVARCHAR(5000),
  total_levels_of_approval NVARCHAR(2),
  Current_level_of_approval NVARCHAR(2),
  Sap_workitem_id NVARCHAR(5000),
  Comments NCLOB,
  submitted_by NVARCHAR(5000),
  submitted_date NVARCHAR(5000),
  PRIMARY KEY(PAN_Number)
) WITH ASSOCIATIONS (
  MANY TO MANY JOIN proj_schema_PAN_WEB_EVENT AS tab1totab2 ON (PAN_Number = tab1totab2.PAN_Number),
  MANY TO MANY JOIN proj_schema_PAN_TYPE AS tab1totab3 ON (PAN_Number = tab1totab3.PAN_Number),
  MANY TO MANY JOIN proj_schema_PAN_WORKFLOW_HISTORY AS tab1toWORKFLOW_HISTORY ON (PAN_Number = tab1toWORKFLOW_HISTORY.PAN_Number),
  MANY TO MANY JOIN proj_schema_PAN_attachments AS tab1topdf ON (tab1topdf.PAN_Number = PAN_Number),
  MANY TO MANY JOIN proj_schema_PAN_vendor_data AS tab1tovendor_data ON (PAN_Number = tab1tovendor_data.PAN_Number),
  MANY TO MANY JOIN proj_schema_PAN_Comments AS tab1tocom ON (PAN_Number = tab1tocom.PAN_Number)
)